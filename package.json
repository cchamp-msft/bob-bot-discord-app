{
  "name": "bob-bot-discord-app",
  "version": "1.0.0",
  "description": "Discord bot with ComfyUI, Ollama, AccuWeather, and NFL integration",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:watch": "nodemon --exec ts-node src/index.ts",
    "register": "ts-node src/registerCommands.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "discord",
    "bot",
    "comfyui",
    "ollama",
    "accuweather",
    "nfl",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "axios": "^1.13.5",
    "discord.js": "^14.14.0",
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@microsoft/eslint-formatter-sarif": "^3.1.0",
    "@types/express": "^5.0.6",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.2.3",
    "@types/ws": "^8.18.1",
    "eslint": "^10.0.0",
    "jest": "^30.2.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.56.0"
  },
  "overrides": {
    "undici": ">=6.23.0",
    "minimatch": ">=10.2.1"
  }
}
